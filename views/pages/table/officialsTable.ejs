<%- include('../../template/header') -%>

<div class="wrapper">
    <%- include('../../template/sidebar') -%>
    <div class="main-panel">
        <%- include('../../template/navbar') -%>
        <div class="content">
            <div class="container-fluid">
              <!-- your content here -->
                <div class="container">
                    <h1 class="text-center" style='margin-top:1em'>Officials Information</h1>
                    <br>
                    
                    <% if(output.length > 0) { %>
                        
                        <table id="myTable" class="table table-striped table-bordered" >
                                <thead>
                                    <tr>
                                        <th>Officials Name</th>
                                        <th>Designation</th>
                                        <th>National Id</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <% output.forEach((entry) => { %>
                                    <tbody>
                                            <tr>
                                                <td><%= entry.basicInfo.employeeName %></td>
                                                <td><%= entry.basicInfo.designation %></td>
                                                <td><%= entry.basicInfo.nationalID %></td>
                                                <td></td>
                                            </tr>
                                    </tbody>
                                <% }); %>
                        </table>
                        
                    <% } else { %>
                        <h5 class="text-center">No Record Found !</h5>
                    <% } %>
                </div>
            </div>
        </div>
        <%- include('../../template/footer') -%>
    </div>
</div>
<%- include('../../template/footerlink') -%>








